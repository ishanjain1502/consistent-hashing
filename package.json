    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.37.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "rimraf": "^4.4.1",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  }
